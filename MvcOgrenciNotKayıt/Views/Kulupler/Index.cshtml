@using MvcOgrenciNotKayıt.Models.EntityFramework
@model List<TBLKULUPLER>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>KULÜP ID</th>
        <th>KULÜP ADI</th>
        <th>KULÜP KONTENJAN</th>
    </tr>
    <tbody>
        @foreach (var klp in Model)
        {
            <tr>
                <td>@klp.KULUPID</td>
                <td>@klp.KULUPAD</td>
                <td>@klp.KULUPKONTENJAN</td>
            </tr>
        }

    </tbody>
</table>

